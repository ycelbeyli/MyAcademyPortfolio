@model About
@{
    ViewData["Title"] = "Yeni Hakkımda Ekle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">

    <div class="form-group">
        <label>Resim URl</label>
        <input type="text" class="form-control" asp-for="ImageUrl" />
    </div>

    <div class="form-group">
        <label>Ünvan</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>

    <div class="form-group">
        <label>Açıklama</label>
        <input type="text" class="form-control" asp-for="Description" />
    </div>

    <div class="form-group">
        <label>Doğum Tarihi</label>
        <input type="text" class="form-control" asp-for="Birthdate" />
    </div>

    <div class="form-group">
        <label>Web Site</label>
        <input type="text" class="form-control" asp-for="WebSite" />
    </div>

    <div class="form-group">
        <label>Telefon</label>
        <input type="text" class="form-control" asp-for="PhoneNumber" />
    </div>

    <div class="form-group">
        <label>Şehir</label>
        <input type="text" class="form-control" asp-for="City" />
    </div>

    <div class="form-group">
        <label>Mezuniyet</label>
        <input type="text" class="form-control" asp-for="Graduation" />
    </div>

    <div class="form-group">
        <label>Email</label>
        <input type="text" class="form-control" asp-for="Email" />
    </div>
    
    
  @*   <div class="form-group">
        <label>Frelancer</label>
        <input type="text" class="form-control" asp-for="IsAvailable" />
    </div>
     *@

    <button class="btn btn-outline-primary">Kaydet</button>

</form>