@model ContactInfo

@{
    ViewData["Title"] = "İletişim Bilgisi Güncelle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">

    <input type="hidden" asp-for="ContactInfoId" />

    <div class="form-group">
        <label>Adres</label>
        <input type="text" class="form-control" asp-for="Address" />
    </div>

    <div class="form-group">
        <label>Cep Telefonu</label>
        <input type="text" class="form-control" asp-for="PhoneNumber" />
    </div>

    <div class="form-group">
        <label>E-Mail</label>
        <input type="text" class="form-control" asp-for="Email" />
    </div>

    <div class="form-group">
        <label>Harita</label>
        <input type="text" class="form-control" asp-for="MapUrl" />
    </div>

    <button class="btn btn-outline-primary">Güncelle</button>

</form>